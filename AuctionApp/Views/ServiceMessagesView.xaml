<?xml version="1.0" encoding="utf-8" ?>
<ContentView xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:view="clr-namespace:AuctionApp.Views"
             x:Class="AuctionApp.Views.ServiceMessagesView"
             x:DataType="view:ServiceMessagesView">
    <VerticalStackLayout>
        <CollectionView
            ItemsSource="{Binding Messages}">
        </CollectionView>
    </VerticalStackLayout>
</ContentView>
